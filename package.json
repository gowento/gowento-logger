{
  "name": "gowento-logger",
  "version": "2.7.0",
  "description": "Gowento Logger",
  "author": "Gowento",
  "contributors": [
    "Eric Redon <eric@gowento.com>",
    "Loris Guignard <loris@gowento.com>"
  ],
  "main": "dist/logger.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test --cache",
    "prebuild": "yarn run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "yarn run test -s",
    "test": "ava",
    "prepublish": "yarn run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "postpublish": "git push origin master --follow-tags"
  },
  "ava": {
    "failFast": false,
    "verbose": false,
    "require": [
      "babel-register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gowento/gowento-logger"
  },
  "license": "MIT",
  "homepage": "https://github.com/gowento/gowento-logger",
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "delay": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-gowento": "4.2.0",
    "mock-express-request": "0.2.2",
    "mock-express-response": "0.2.2",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "destroy-circular": "1.1.0",
    "figures": "2.0.0",
    "flat": "4.1.0",
    "lodash": "4.17.10",
    "logfmt": "1.2.1",
    "request-ip": "2.1.1"
  }
}
