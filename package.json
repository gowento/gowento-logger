{
  "name": "gowento-logger",
  "version": "2.1.0",
  "description": "Gowento Logger",
  "author": "Gowento",
  "contributors": [
    "Eric Redon <eric@gowento.com>",
    "Loris Guignard <loris@gowento.com>"
  ],
  "main": "dist/logger.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test --cache",
    "prebuild": "yarn run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "yarn run test -s",
    "test": "ava",
    "prepublish": "yarn run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "postpublish": "git push origin master --follow-tags"
  },
  "ava": {
    "failFast": false,
    "verbose": false,
    "require": [
      "babel-register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:gowento/logger.git"
  },
  "license": "MIT",
  "homepage": "https://bitbucket.org/gowento/logger#readme",
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "eslint": "4.19.1",
    "eslint-config-gowento": "4.2.0",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "heroku-logger": "loris/heroku-logger#83856e4",
    "lodash": "4.17.5"
  }
}
